{
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:test": "tsc -b && vite build --mode test",
    "build:prod": "npm run build",
    "lint": "eslint .",
    "format": "prettier --check .",
    "preview": "vite preview",
    "docker:dev:start": "docker compose -f environments/dev/docker-compose.yml --env-file environments/dev/.env.development up --build",
    "docker:dev:stop": "docker compose -f environments/dev/docker-compose.yml --env-file environments/dev/.env.development down",
    "docker:test:start": "docker compose -f environments/test/docker-compose.yml --env-file environments/test/.env.test up --build",
    "docker:test:stop": "docker compose -f environments/test/docker-compose.yml --env-file environments/test/.env.test down",
    "docker:preprod:start": "docker compose -f environments/preprod/docker-compose.yml --env-file environments/preprod/.env.preprod up --build",
    "docker:preprod:stop": "docker compose -f environments/preprod/docker-compose.yml --env-file environments/preprod/.env.preprod down",
    "docker:prod:start": "docker compose -f environments/prod/docker-compose.yml --env-file environments/prod/.env.production up --build",
    "docker:prod:stop": "docker compose -f environments/prod/docker-compose.yml --env-file environments/prod/.env.production down"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.14.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.4.12"
  }
}
